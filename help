#include <iostream>
#include <cmath>
using namespace std;

int main()
{
	long n;
	char flag;
	flag = 1;
	cin >> n;
	for (long i = 2; i < pow(n, 0.5) + 1; i++)
	{
		if (n % i == 0)
		{
			for (long j = 2; j < pow(i, 0.5) + 1; j++)
			{
				if (i % j == 0 && i != j)
				{
					flag = 0;
					break;
				}
			}
			if(flag == 0)
			{
				break;
			}
			while (n % i == 0)
			{
				n /= i;
				cout << i << endl;
			}
		}
	}
}
