#include <iostream>
#include <math.h>
#include <cmath>
using namespace std;

int main()
{
	int x;
	cin >> x;
	int x1 = x % 10; //вторая цифра
	int x2 = x / 10; //первая цифра
	x1 <= x2 ? cout << "first" : cout << "second"; //вывод с использованием тернарного оператора
}



//ПРИМЕР 1 21 вывод: first

//ПРИМЕР 2 37 вывод: second

//ПРИМЕР 3 99 вывод: first
