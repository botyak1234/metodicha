#include <iostream>
#include <cmath>
#include <iomanip>
#include <windows.h>
using namespace std;
int main()
{
	int x, y;
	cin >> x >> y;
	for (int i = 1; i <= 4; i++)
	{
		for (int j = 1; j <= i; j++) //один цикл для строк, начиная, с 1, второй для строк, начиная с 6
		{
			cout << x << "  ";
		}
		cout << endl;
		for (int j = 1; j <= i; j++)
		{
			cout << y << "  ";
		}
		cout << endl;
		x++;
		y++;
	}
}
