#include <iostream>
#include <cmath>
#include <iomanip>
#include <windows.h>
using namespace std;

void f(double &a, int &b, int &fl)
{
	if (b * b + 2 * b + 1 > 0)
	{
		a = (3. * b + 4)/sqrt(b * b + 2 * b + 1);
	}
	else fl = 0;
}


int main()
{
	SetConsoleCP(1251);
	SetConsoleOutputCP(1251);
	long aa, bb, h;
	int flag;
	double y = 0;
	cin >> aa >> bb >> h;
	for (int i = aa; i <= bb; i += h )
	{
		flag = 1;
		f(y, i, flag);
		if (!flag) cout << i <<"  Функция не определена" << endl;
		else cout << i << "  " << y << endl;
	}
}

