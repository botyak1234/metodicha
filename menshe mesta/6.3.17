#include <iostream>
#include <cmath>
#include <iomanip>
#include <windows.h>
#include <vector>
#include <string>
using namespace std;



int main()
{
	int flag = 0;
	int fl = 0;
	long n; cin >> n;
	long **b = new long *[n];
	for (long i = 0; i < n; i++)
	{
		for(long j = 0 ; j < n; j++)
		cin >> b[i][j];
	}
	for (long i = 0; i < n; i++)
	{
		for (long j = 0; j < n; j++)
		{
			if (b[i][j] < 0)
			{
				fl = 1;
				break;
			}
		}
		if (fl)
		{
			flag = 1;
			break;
		}
	}
	if (flag) cout << "No";
	else cout << "Yes";
}
