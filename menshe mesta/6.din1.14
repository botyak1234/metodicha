#include <iostream>
#include <cmath>
#include <iomanip>
#include <windows.h>
#include <vector>
#include <string>
using namespace std;



int main()
{
	long n; cin >> n;
	long *array = new long[n];
	long mx = 1;
	for (int i = 0; i < n; i++)
	{
		cin >> array[i];
	}
	for (int i = 0; i < n; i++)
	{
		if (array[i] < 0 && abs(array[i]) > mx)
		mx = array[i];
	}
	cout << mx;
}
