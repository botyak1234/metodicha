#include <iostream>
#include <cmath>
#include <iomanip>
#include <windows.h>
#include <algorithm>
#include <vector>
#include <string>
#include <sstream>
#include <cstring>
#include <fstream>
#include <vector>
using namespace std;

struct student
{
	string imya, fam, otchestvo;
	int god;
	int marks[5];
};

void puzir(student* a, int n)
{
	for (int i = 0; i < n - 1; i++)
	{
		for (int j = n - 1; j > i; j--)
		{
			if (a[j].fam > a[j - 1].fam)
			{
				swap(a[j], a[j - 1]);
			}
		}
	}
}

void vibor(student *a, int n)
{
	string lowkey;
	string lowkeyint;
	int lowindex;
	for (int i = 0; i < n - 1; i++)
	{
		lowindex = i;
		lowkey = a[i].fam;
		for (int j = i + 1; j < n; j++)
		{
			if (a[j].fam < lowkey)
			{
				lowkey = a[j].imya;
				lowindex = j;
			}
		}
		swap(a[i], a[lowindex]);
	}
}



int main()
{
	
	int n; cin >> n;
	ifstream in("input.txt");
	ofstream out("output.txt");
	student* array = new student[n];
	while (in.peek() != EOF)
	{
		for (int i = 0; i < n; i++)
		{
			in >> array[i].imya >> array[i].fam >> array[i].otchestvo >> array[i].god;
			for (int j = 0; j < 5; j++)
			{
				in >> array[i].marks[j];
			}
		}
	}
	vibor(array, n);
	for (int i = 0; i < n; i++)
	{
		out << array[i].imya << ' ' << array[i].fam << ' ' << array[i].otchestvo << ' ' << array[i].god << ' ';
		for (int j = 0; j < 5; j++)
		{
			out << array[i].marks[j] << ' ';
		}
		out << endl;
	}
}
