#include <cmath>
#include <iomanip>
#include <windows.h>
#include <algorithm>
#include <vector>
#include <string>
#include <sstream>
#include <cstring>
#include <fstream>
#include <vector>
using namespace std;


class novoe
{
private:
		int* IntArray = new int[n];
public:
	void operator ++()
	{
		for (int i = 0; i < n; i++) IntArray[i]++;
	}
	void operator *(int scalyar)
	{
		for (int i = 0; i < n; i++) IntArray[i] * scalyar;
	}
		int n;
		void vvod(int n)
		{
			for (int i = 0; i < n; i++) cin >> IntArray[i];
		}
		void vivod(int n)
		{
			for (int i = 0; i < n; i++) cout << IntArray[i] << ' ';
		}
		void sortirovka(int n)
		{
			sort(IntArray[0], IntArray[n]);
		}
		int razmer()
		{
			return sizeof(IntArray);
		}
		double summa(int n)
		{
			int s = 0;
			for (int i = 0; i < n; i++) s += IntArray[i];
			return s;
		}
};



int main()
{
	int a; cin >> a;
	novoe firstObject;
	novoe secondobject;
	novoe tobject;
	secondobject.n = a;
	firstObject.vvod(secondobject.n);
	secondobject.vivod(secondobject.n);
}
