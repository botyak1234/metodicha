class novoe
{
private:
	int* IntArray;
public:
	int n;
	novoe() {}
	/*novoe(int n)
	{
		IntArray = new int[n];
	}*/
	void resize(int n)
	{
		IntArray = new int[n];
	}
	/*void operator ++()
	{
		for (int i = 0; i < n; i++) ++IntArray[i];
	}
	void operator ++(int)
	{
		for (int i = 0; i < n; i++) ++IntArray[i];
	}*/
	void operator *(int scalyar)
	{
		for (int i = 0; i < n; i++) IntArray[i] *= scalyar;
	}
	/*void operator == (int* arr)
	{
		for (int i = 0; i < n; i++)
		{
			arr[i] = IntArray[i];
		}
	}*/
	void vvod()
	{
		cin >> n;
		for (int i = 0; i < n; i++) cin >> IntArray[i];
	}
	void vivod(int n)
	{
		for (int i = 0; i < n; i++) cout << IntArray[i] << ' ';
		cout << endl;
	}
	void sortirovka(int n)
	{
		int lowkey;
		int lowkeyint;
		int lowindex;
		for (int i = 0; i < n - 1; i++)
		{
			lowindex = i;
			lowkey = IntArray[i];
			for (int j = i + 1; j < n; j++)
			{
				if (IntArray[j] < lowkey)
				{
					lowkey = IntArray[j];
					lowindex = j;
				}
			}
			swap(IntArray[i], IntArray[lowindex]);
		}
	}
	int razmer()
	{
		return sizeof(IntArray);
	}
	double summa(int n)
	{
		int s = 0;
		for (int i = 0; i < n; i++) s += IntArray[i];
		return s;
	}
};



int main()
{
	int a; cin >> a;
	novoe* arr = new novoe[a];

	for (int i = 0; i < a; i++)
	{
		arr[i].resize(a);
	}
	for (int i = 0; i < a; i++)
	{
		arr[i].vvod();
		arr[i].vivod(arr[i].n);
	}
	
	/*novoe f;
