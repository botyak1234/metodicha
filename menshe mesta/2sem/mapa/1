#include <iostream>
#include <cmath>
#include <iomanip>
#include <windows.h>
#include <algorithm>
#include <vector>
#include <string>
#include <sstream>
#include <cstring>
#include <fstream>
#include <map>
#include <vector>

using namespace std;


bool is_number(const string& s) 
{
    for (char c : s) 
    {
        if (!isdigit(c)) 
        {
            return false;
        }
    }
    return true;
}


int main() {
    ifstream in("input.txt");
    map<int, vector<int>> positions;
    string line;
    while (getline(in, line)) 
    {
        istringstream iss(line);
        string word;
        int position = 1;
        while (iss >> word) 
        {
            if (is_number(word))
            {
                int number = stoi(word);
                positions[number].push_back(position);
                position++;
            }
        }
    }

    for (const auto& p : positions) 
    {
        cout << p.first << endl;
        for (const auto& pos : p.second) 
        {
            cout << pos << " ";
        }
        cout << endl;
    }
}
