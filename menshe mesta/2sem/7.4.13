#include <iostream>
#include <cmath>
#include <iomanip>
#include <windows.h>
#include <algorithm>
#include <vector>
#include <string>
#include <sstream>
#include <cstring>
#include <fstream>
#include <vector>
using namespace std;



int main()
{
	string s;
	getline(cin, s);
	int chislo = abs(stoi(s));
	int sum = 0;
	while (chislo)
	{
		sum += chislo % 10;
		chislo /= 10;
	}
	cout << sum;
}
	
