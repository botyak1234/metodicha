template <typename T> double sred(T* a, int n)
{
	T s = 0;
	for (int i = 0; i < n; i++)
	{
		s += a[i];
	}
	return s / (double)n;
}

template <typename T> void vvod(T* a, int n)
{
	for (int i = 0; i < n; i++) cin >> a[i];
}

template <typename T> void vivod(T* a, int n)
{
	for (int i = 0; i < n; i++)
	{
		cout << a[i] << ' ';
	}
}

int main()
{
	int n; cin >> n;
	double* a = new double[n];
	vvod(a, n);
	cout << sred(a, n);
}
