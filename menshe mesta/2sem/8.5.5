template <typename T> double sred(T* a, int n)//шаблон функция вычисления среднего значения элементов массива где типом данных есть T
{
	T s = 0;
	for (int i = 0; i < n; i++)
	{
		s += a[i];
	}
	return s / (double)n;
}

template <typename T> void vvod(T* a, int n)//шаблон функция ввода массива
{
	for (int i = 0; i < n; i++) cin >> a[i];
}

template <typename T> void vivod(T* a, int n)//шаблон функция вывода массива
{
	for (int i = 0; i < n; i++)
	{
		cout << a[i] << ' ';
	}
}

int main()
{
	int n; cin >> n;
	double* a = new double[n];
	int* b = new int[n];
	vvod(a, n);
	vvod(b, n);
	cout << sred(a, n) << endl;
	cout << sred(b, n) << endl;
}
