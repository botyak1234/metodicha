float F(float x)
{
	float y;
	long eps = 1e-12;
	if (abs(x - 0.1) <= eps)
	{
		y = pow(x, 3) - 0.1;
		return y;
	}
	else if (abs(x - 0.2) <= eps && abs(x) >= 0.1)
	{
		y = 0.2 * x - 0.1;
		return y;
	}
	else
	{
		y = pow(x, 3) + 0.1;
		return y;
	}
}

