float F(float x)
{
	if (abs(x) <= 0.1) return pow(x, 3) - 0.1;
	if (abs(x) <= 0.2 && abs(x) >= 0.1) return 0.2 * x - 0.1;
	return pow(x, 3) + 0.1;
}
