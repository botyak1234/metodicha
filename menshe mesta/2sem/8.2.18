using namespace std;

int rek(int* a, int n, int &m)
{
	if (n > 0)//условие выхода из рекурсии
	{
		if (a[n - 1] > m ) m = a[n - 1];//поиск максимального
		return rek(a, n - 1, m);//вызов функции самой себя, то есть вызов рекурсии
	}
	return m;
}

int main()
{
	int n; cin >> n;
	int m = -100000;
	int* a = new int[n];
	for (int i = 0; i < n; i++) cin >> a[i];
	cout << rek(a, n, m);
}
