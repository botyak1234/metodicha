#include <iomanip>
#include <windows.h>
#include <algorithm>
#include <vector>
#include <string>
#include <sstream>
#include <cstring>
#include <fstream>
#include <vector>

using namespace std;
long m = -1000000;

int rek(int* a, int n)
{
	if (n > 0)
	{
		if (a[n - 1] > m) m = a[n - 1];
		return rek(a, n - 1);
	}
	return m;
}

int main()
{
	int n; cin >> n;
	int* a = new int[n];
	for (int i = 0; i < n; i++) cin >> a[i];
	cout << rek(a, n);
}
