#include <iostream>
#include <cmath>
#include <iomanip>
#include <windows.h>
#include <algorithm>
#include <vector>
#include <string>
using namespace std;



int main()
{
	char c2; cin >> c2;
	string s; cin >> s;
	string s0; cin >> s0;
	int len = size(s0);
	char zabyl = s[size(s) - 1];
	int c = count(s.begin(), s.end(), c2);
	s.insert(size(s), c*len, '1');
	int lens = size(s);
	for (int i = 0; i < lens; i++)
	{
		if (s[i] == c2)
		{
			for (int j = lens - 1; j > i + 1; j--)
			{
				s[j] = s[j - 1];
			}
			s.insert(i + 1, s0);
		}
	}
	s.erase(size(s) - lens+2, lens);
	s[lens-1] = zabyl;
	cout << s;
}
