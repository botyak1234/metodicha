#include <iostream>
#include <cmath>
#include <iomanip>
#include <windows.h>
#include <vector>
#include <string>
using namespace std;



int main()
{
	long a[10000];
	long n; cin >> n;
	for (int i = 0; i < n; i++)
	{
		cin >> a[i];
	}
	for (int i = 0; i <= n; i++)
	{
		if (a[i] % 2 != 0)
			cout << i + 1 << ' ';
	}
}
