#include <iostream>
#include <cmath>
#include <iomanip>
#include <windows.h>
#include <vector>
#include <string>
using namespace std;



int main()
{
	long n; cin >> n;
	long *array = new long[n];
	long mn = 1000000000000000;
	for (int i = 0; i < n; i++)
	{
		cin >> array[i];
	}
	for (int i = 0; i < n; i++)
	{
		if (array[i] < mn)
		{
			mn = array[i];
		}
	}
	for (int i = 0; i < n; i++)
	{
		if (array[i] == mn)
		{
			cout << i + 1 << ' ';
		}
	}
}
