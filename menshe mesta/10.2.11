#include <iostream>
#include <cmath>
#include <iomanip>
#include <windows.h>
#include <algorithm>
#include <vector>
#include <string>
#include <cstring>
#include <fstream>
using namespace std;


	ifstream in("input.txt");
	ofstream out("output.txt");

	struct point
	{
		string vid;
		int cost;
		string sort;
		int kolvo;
		void f();
	};
	void point::f()
	{
		out << vid << cost << sort;
	}
int main()
{
	int n; in >> n;
	point* a = new point[n];
	int kolv = 100000000;
	int ind;
	for (int i = 0; i < n; i++)
	{
		in >> a[i].vid;
		in >> a[i].cost;
		in >> a[i].kolvo;
	}
	for (int i = 0; i < n; i++)
	{
		if (a[i].kolvo < kolv)
		{
			kolv = a[i].kolvo;
			ind = i;
		}
	}
	out << a[ind].vid << a[ind].cost << a[ind].sort;
	in.close();
	out.close();
}
