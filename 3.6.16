#include <iostream>
#include <cmath>
#include <iomanip>
#include <windows.h>
using namespace std;

double a(double x)
{
	double eps = 1e-12;
	if (x > eps) return x;
	else if (x <= eps && x +1 >= eps) return 0;// проверка всех трёх с условий с очень маленьким числом
	else return x * x;
}






int main()
{
	double aa, bb, h;
	double y = 0;
	double eps = 1e-12;
	cin >> aa >> bb >> h;
	for (double i = aa; i - bb <= eps; i += h)
	{
		cout << i << ' ' << a(i) << '\n';
	}
}



