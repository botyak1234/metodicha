#include <iostream>
#include <cmath>
#include <iomanip>
using namespace std;
//выше было произведено подключение бибиотек потоков ввода и вывода данных, математических функций, манипуляций над потоком вывода и пространство имён std
int main()
{
	long double x; //объяявление переменной
	cin >> x;//ввод переменной
	cout << fixed << setprecision(10) << abs(x * x * x - x * x) - 7. * x / (x * x * x - 15 * x);/*вывод ответа с 10 знаками после запятой, abs - модуль числа,
	7. - перевод числа в тип double, т.к. мне нужно не целочисленное деление  */
}




